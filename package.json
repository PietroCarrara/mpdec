{
  "name": "mpdec",
  "version": "1.0.0",
  "description": "Electron-based mpd client",
  "scripts": {
    "build": "tsc && sass view/themes/:themes/ && electron-rebuild",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js"
  },
  "repository": "https://github.com/PietroCarrara/mpdec",
  "keywords": [
    "Electron",
    "typescript",
    "mpd",
    "music-player"
  ],
  "author": "Pietro Carrara",
  "devDependencies": {
    "electron": "9.4.0",
    "electron-rebuild": "^1.8.5",
    "sass": "^1.22.3",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@types/sharp": "^0.22.2",
    "bootstrap": "^4.3.1",
    "material-design-icons": "^3.0.1",
    "mpc-js": "^1.2.1",
    "sharp": "^0.22.1"
  }
}
